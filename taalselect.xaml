        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Keyboard_Tester"
        mc:Ignorable="d"
        Title="taalselect" Height="450" Width="800">
    <Grid>
        <Button Content="Nederlands" HorizontalAlignment="Left" Margin="22,118,0,0" VerticalAlignment="Top" Click="NederlandsButton_Click"/>
        <Button Content="Français" HorizontalAlignment="Left" Margin="122,118,0,0" VerticalAlignment="Top" Click="FransButton_Click"/>
        <Button Content="Español" HorizontalAlignment="Left" Margin="207,118,0,0" VerticalAlignment="Top" Click="SpaansButton_Click"/>
        <Label Content="Hello" HorizontalAlignment="Left" Margin="35,92,0,0" VerticalAlignment="Top"/>
        <Label Content="Bonjour" HorizontalAlignment="Left" Margin="127,92,0,0" VerticalAlignment="Top"/>
        <Label Content="Hola" HorizontalAlignment="Left" Margin="212,92,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
